{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Response",
    "description": "服务器响应格式",
    "type": "object",
    "properties": {
        "code": {
            "description": "状态码 成功:1, 失败: 0",
            "type": "string"
        },
        "message": {
            "description": "提示消息",
            "type": "string"
        },
        "result": {
            "description": "结果内容",
            "type": "object"
        }
    },
    "required": ["code", "result"]
}
